    {
      "name": "comments",
      "path": "/comments",
      "primary_keys": ["id"],
      "replication_key": "id",
      "records_path": "$[*]",
      "params": {}
    }
  ],
  "offline_discovery": true,
  "debug_logging": "INFO"
}
